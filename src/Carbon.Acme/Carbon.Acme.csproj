<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Carbon.Acme</AssemblyName>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.20.0</Version>
    <LangVersion>8.0</LangVersion>
    <NullableContextOptions>enable</NullableContextOptions>
    
    <!-- Package Details -->
    <Authors>iamcarbon</Authors>
    <Copyright>© Jason Nelson</Copyright>
    <Description>An ACME V2 client for requesting certificates through Let's Encrypt and other ACME providers.</Description>

    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>acme;acmev2;carbon;netstandard</PackageTags>
    <PackageProjectUrl>https://github.com/carbon/Acme</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Carbon.Jose" Version="0.18.0" />
  </ItemGroup>

</Project>